class ch2_child2 extends ch2{
  ch2_child2(float p, float q, float r, int op2){
    super(p,q,r,op2);
  }
  void display(){
    bonobono();
    fill(256,256,256);
    circle(x-d*23,y-d*10, d*10);
    circle(x+d*23,y-d*10, d*10);
    fill(0, 0, 0);
    stroke(0, 0, 0);
    circle(x-d*22, y-d*10, d*4);
    circle(x+d*22, y-d*10, d*4);
  }
 void baggue(){
    super.baggue();
  }
}
